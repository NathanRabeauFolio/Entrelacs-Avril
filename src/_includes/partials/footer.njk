  const modalphone = document.getElementById('modalphone')
const active = document.getElementById('active-modalphone')


const initmodalphone = () => {

    modalphone.classList.add('display')
    modalphone.addEventListener('click', (e) => {
        if(e.target.id == 'modalphone' || e.target.id == 'close'){
            modalphone.classList.remove('display')
        }
    })
}


active.addEventListener('click', initmodalphone)

